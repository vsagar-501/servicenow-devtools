        <value>var strLog = '';&#13;
function Log(strText)&#13;
{&#13;
	strLog = strLog + "\n" + strText;&#13;
}&#13;
(function(outputs, steps, stepResult, assertEqual)&#13;
{&#13;
	var RenderValue = x_snc_devtools.RenderValue;&#13;
&#13;
	var bResult = true;&#13;
	&#13;
&#13;
	function TestCase_IsValidVersion(vInput, vExpected)&#13;
	{&#13;
		Log('TestCase_IsValidVersion');&#13;
		Log('Input: '+RenderValue(vInput));&#13;
		Log('Expected: '+RenderValue(vExpected));&#13;
		var vResult = IsValidVersion(vInput);&#13;
		Log('Result  : '+RenderValue(vResult));&#13;
		if (vResult != vExpected)&#13;
		{&#13;
			bResult = false;&#13;
			Log('Test case FAILED');&#13;
		}&#13;
		Log('');&#13;
	}&#13;
	&#13;
	TestCase_IsValidVersion('1',true);&#13;
	TestCase_IsValidVersion('1.0',true);&#13;
	TestCase_IsValidVersion('1.1.1',true);&#13;
	TestCase_IsValidVersion('1.1.1',true);&#13;
&#13;
	TestCase_IsValidVersion(null,false);&#13;
	TestCase_IsValidVersion('',false);&#13;
	TestCase_IsValidVersion('a',false);&#13;
	TestCase_IsValidVersion('1.0a',false);&#13;
	TestCase_IsValidVersion('!',false);&#13;
&#13;
	&#13;
	stepResult.setOutputMessage(strLog);&#13;
	return bResult;&#13;
	&#13;
})(outputs, steps, stepResult, assertEqual);</value>
